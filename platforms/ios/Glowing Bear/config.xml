<?xml version='1.0' encoding='utf-8'?>
<widget id="com.glowing_bear" ios-CFBundleIdentifier="org.glowing-bear" version="0.6.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
